        <shape>
            <corners android:radius="3.0dip" />
            <solid android:color="@color/color_space_global_fade_2" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="3.0dip" />
                <gradient android:startColor="#ff005ccc" android:endColor="#ff46c7fd" android:angle="0.0" />
            </shape>
        </clip>
    </item>
</layer-list>